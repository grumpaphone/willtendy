{
	"mcpServers": {
		"sequential-thinking": {
			"command": "npx",
			"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
			"env": {}
		},
		"strapi": {
			"command": "npx",
			"args": ["-y", "@bschauer/strapi-mcp-server", "--stdio"],
			"env": {
				"STRAPI_URL": "http://localhost:1337",
				"STRAPI_API_TOKEN": "your_strapi_api_token_here"
			}
		}
	}
}
